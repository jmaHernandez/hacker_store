<div class="page-header">
  <h1>Edit Order</h1>
</div>

<%= form_for [@user, @order], html: { class: "form-horizontal generation" } do |f| %>
	<%= render 'form', f: f %>

	<div class="form-group">
		<div class="col-lg-offset-2 col-lg-10">
			<%= f.submit "Update", class: 'btn btn-primary' %>
			<%= link_to "Cancel",  user_orders_path(@user), class: 'btn btn-default' %>
		</div>
	</div>
<% end %>
